#include <stdio.h>

struct student
{
    char name[100];
    int rollno;
    int marks;
};

int main()
{
    struct student s;

    printf("Enter the name ");
    scanf(" %[^\n]", s.name); 

    printf("Enter roll number ");
    scanf("%d", &s.rollno);

    printf("Enter the marks");
    scanf("%d", &s.marks);

    printf("\nStudent Details:\n");
    printf("Name: %s\n", s.name);
    printf("Roll Number: %d\n", s.rollno);
    printf("Marks: %d\n", s.marks);

    return 0;
}
